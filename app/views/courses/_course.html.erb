<div id="<%= dom_id course %>">
</div>
<div class="card text-bg-success mb-3" style="max-width: 18rem;" >
  <div class="card-header">
    <strong>Title:</strong>
    <%= course.title %>
  </div>
  <div class="card-body">
    <p>
      <strong>Department:</strong>
      <%= course.department.name %>
    </p>
    <p>
      <strong>Code:</strong>
      <%= course.code %>
    </p>
    <p>
      <strong>Description:</strong>
      <%= course.description %>
    </p>
    <p>
      <strong>Credits:</strong>
      <%= course.credits %>
    </p>
  </div>
</div>

